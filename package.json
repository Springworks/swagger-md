{
  "name": "swagger-md",
  "description": "Converts Swagger API spec to Markdown file",
  "main": "lib/swagger-converter.js",
  "private": false,
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib",
    "commit": "commit-wizard",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint --cache .",
    "prepublish": "npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "NODE_ENV=test istanbul cover _mocha"
  },
  "dependencies": {
    "markdown-link": "0.1.1",
    "toc-md": "0.2.0"
  },
  "devDependencies": {
    "@springworks/test-harness": "1.3.11",
    "babel-cli": "6.10.1",
    "babel-eslint": "6.1.0",
    "babel-preset-es2015-node4": "2.1.0",
    "babel-register": "6.9.0",
    "eslint": "2.13.1",
    "eslint-config-springworks": "7.0.4",
    "eslint-plugin-import": "1.9.2",
    "eslint-plugin-mocha": "3.0.0",
    "eslint-plugin-should-promised": "1.0.8",
    "eslint-plugin-springworks": "2.0.1",
    "fixture-loader": "1.0.0",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "2.5.3",
    "pre-git": "3.9.1",
    "semantic-release": "4.3.5"
  },
  "config": {
    "pre-git": {
      "commit-msg": "conventional"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Springworks/swagger-md.git"
  },
  "keywords": [
    "swagger",
    "markdown",
    "converter",
    "api"
  ],
  "author": "Springworks (http://springworks.se)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Springworks/swagger-md/issues"
  },
  "homepage": "https://github.com/Springworks/swagger-md#readme"
}
